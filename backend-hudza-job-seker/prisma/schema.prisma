// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model U {
  id        String @id
  nama_user String
  alamat    String
  role      String
  email     String @unique
  password  String
}

model J {
  id String  @id
  nj String?
  np String?
  su String?
  dp String?
  lj String?
  tj String?
  s  Int?
  mk String?
}
